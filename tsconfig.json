{
    "compilerOptions": {
        "jsx": "react-jsx",
        "jsxImportSource": "react-auto-clsx",
        "typeRoots": ["node_modules/@types", "./types"],
        "types": ["react", "react-dom", "webpack-env"],
        "esModuleInterop": true,
        "baseUrl": ".",
        "paths": {
            "@/types/*": ["./types/*"],
            "@/assets/*": ["./assets/*"],
            "@/ui/*": ["./src/*"],
            "@/theme/*": ["./theme/*"]
        }
    },
    "include": [
        "./rspress.config.ts",
        "docs/**/*",
        "src/**/*",
        "theme/**/*",
        "types/**/*"
    ]
}
